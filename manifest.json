{
  "manifest_version": 3,
  "name": "Google CN DevSites",
  "description": "When available, automatically maps Google devsites to its CN mirrors",
  "version": "0.26",
  
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "host_permissions": [
    "*://*.google.com/*",
    "*://*.android.com/*",
    "*://*.tensorflow.org/*",
    "*://*.tfhub.dev/*",
    "*://*.angular.io/*",
    "*://*.golang.org/*",
    "*://*.gstatic.com/*",
    "*://*.googleapis.com/*",
    "*://*.google.cn/*",
    "*://*.gstatic.cn/*"
  ],
  
  "background": {
    "service_worker": "bg.js"
  },
  
  "icons": {
    "128": "on.png"
  },
  
  "action": {}
}
